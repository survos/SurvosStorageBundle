{
    "name": "survos/storage-bundle",
    "description": "Symfony bundle for interacting with storage (via Flysystem)",
    "keywords": [
        "symfony-ux",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "twig/twig": "^3.4",
        "league/flysystem-bundle": "^3.5",
        "zenstruck/bytes": "^1.0"
    },
    "require-dev": {
        "league/flysystem-aws-s3-v3": "^3.29",
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3||^8.0",
        "symfony/framework-bundle": "^7.3||^8.0",
        "symfony/phpunit-bridge": "^7.3||^8.0",
        "symfony/twig-bundle": "^7.3||^8.0",
        "symfony/var-dumper": "^7.3||^8.0",
        "survos/simple-datatables-bundle": "^1.5",
        "symfony/yaml": "^7.3||^8.0",
        "symfony/http-client": "^7.3||^8.0",
        "phpunit/phpunit": "^12",
        "symfony/messenger": "^7.3||^8.0",
        "symfony/console": "^7.3||^8.0",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Survos\\StorageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\StorageBundle\\Tests\\": "Tests/"
        }
    },
    "suggest": {
        "survos/simple-datatables-bundle": "searchable/sortable tables when browsing"
    }
}
